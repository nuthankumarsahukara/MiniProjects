<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
<hibernate-mapping>
<class name="com.tmf.course.hibernataebackend.hibernate_course.Course" table="course">
<id name="courseId" column="courseId" type="long"></id>
<property name="courseName" type="string" column="courseName"/>
<property name="courseDuration" type="int" column="courseDuration"/>
<property name="courseFee" type="double" column="courseFee"/>
</class>
</hibernate-mapping>